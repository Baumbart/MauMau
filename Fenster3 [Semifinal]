    Fenster3.jButton23.setVisible(false);
    Fenster3.jButton24.setVisible(false);
    Fenster3.jButton25.setVisible(false);
    Game.interagiert= true;
  } // end of jButton7_ActionPerformed
  
  public void jButton8_ActionPerformed(ActionEvent evt) {
    // TODO hier Quelltext einfügen
    Game.input=3;//Setzt input auf 3, womit die Nachfolgene Abfrage das Enthüllen des Knopfes selbst(jButton8) und das Enthüllen des Vorangengangenen(jButton7), sowie wiederum dessen vorgänger zur Folge(bis jButton6) hat(Beschreibung hier fast identisch lediglich jew. ein Knopf mehr Pro gehaltener Karte)(wie erwähn wiederholt sich das Prozedere und wird deshalb nicht einzeln Beschriftet)
    Fenster3.jButton6.setVisible(false);
    Fenster3.jButton7.setVisible(false);
    Fenster3.jButton8.setVisible(false);
    Fenster3.jButton9.setVisible(false);
    Fenster3.jButton10.setVisible(false);
