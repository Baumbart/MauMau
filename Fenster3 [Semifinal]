  public void jButton2_ActionPerformed(ActionEvent evt) {
    // TODO hier Quelltext einfügen
    if(pressCounter== 0){//Prüft ob der Knopf bereits in Gebrauch war (hier überall Gleiche Beschreibung)
      Game.input= 2;//Setzt input auf 2 um die Option in der nachfolgenden Bedingung/Abfrage ind richtung "Zwei Spieler" abbiegen zu lassen
      Game.interagiert= true;//Setzt die (Nachfolgende) Bedingung der "Warteschleife" auf erfüllt (Hält die Threads Synchron da sonst Game zu schnell agieren würde)(hier überall Gleiche Beschreibung)
    }
    
    if (pressCounter== 1) {//Wenn der Knopf bereits in Gebrauch war wurde der Code in der Vorrangegangenen Verzweigung bereits konfiguriert bzw soll nun etwas anderes konfiguriert werden
      Game.input= 1;//Setzt input auf 1 um die Option in der nachfolgenden Bedingung/Abfrage ind richtung "Spieler 1 beginnt" abbiegen zu lassen
      Game.interagiert= true;
    } // end of if
    
  } // end of jButton2_ActionPerformed
  
  public void jButton3_ActionPerformed(ActionEvent evt) {
    // TODO hier Quelltext einfügen
    if(pressCounter== 0){
      Game.input= 3;//Lässt(Nachfolgende)Bedingung in Richtung "Drei Spieler" abzweigen
      Game.interagiert= true;
    }
    
    if (pressCounter== 1) {
      Game.input= 2;//Lässt die nachfolgende Bedingung in Richtung "Spieler 2 beginnt" abbiegen
      Game.interagiert= true;     
    } // end of if
    
  } // end of jButton3_ActionPerformed
  
  public void jButton1_ActionPerformed(ActionEvent evt) {
    // TODO hier Quelltext einfügen
    if (pressCounter== 1) {  
      Game.input= 3;//Lässt die nachfolgende Bedingung in Richtung "Spieler 3 beginnt" abbiegen   
      Game.interagiert= true;
    } // end of if
  } // end of jButton1_ActionPerformed
  
  public void jButton4_ActionPerformed(ActionEvent evt) {
    // TODO hier Quelltext einfügen
    Game.input=1;//Lässt die nachfolgende Bedingung das Ablegen einer Karte ansteuern
    jButton4.setVisible(false);//Verbirgt jButton4(Hier wiederholen sich die erklärungen ebenfalls (Lediglich variablennamen ändern sich)) 
    jButton5.setVisible(false);//Verbirgt jButton5
    Game.interagiert= true;     
  } // end of jButton4_ActionPerformed
  
  public void jButton5_ActionPerformed(ActionEvent evt) {
    // TODO hier Quelltext einfügen
    Game.input=2;//Lässt die nachfolgende Bedingung das Aufnemen einer Karte zur Folge haben
    jButton5.setVisible(false);
    jButton4.setVisible(false);
    Game.interagiert= true;
  } // end of jButton5_ActionPerformed
  
  public void jButton6_ActionPerformed(ActionEvent evt) {
    // TODO hier Quelltext einfügen
    Game.input=1;//Setzt input auf 1, womit die Nachfolgene Abfrage das Enthüllen des Knopfes(jButton6) zur Folge hat(Beschreibung hier fast identisch lediglich jew. ein Knopf mehr Pro gehaltener Karte)
    Fenster3.jButton6.setVisible(false);
    Fenster3.jButton7.setVisible(false);
    Fenster3.jButton8.setVisible(false);
    Fenster3.jButton9.setVisible(false);
    Fenster3.jButton10.setVisible(false);
    Fenster3.jButton11.setVisible(false);
    Fenster3.jButton12.setVisible(false);
    Fenster3.jButton13.setVisible(false);
    Fenster3.jButton14.setVisible(false);
    Fenster3.jButton15.setVisible(false); 
    Fenster3.jButton16.setVisible(false);
    Fenster3.jButton17.setVisible(false);
    Fenster3.jButton18.setVisible(false);
    Fenster3.jButton19.setVisible(false);
    Fenster3.jButton20.setVisible(false);
    Fenster3.jButton21.setVisible(false);
    Fenster3.jButton22.setVisible(false);
    Fenster3.jButton23.setVisible(false);
    Fenster3.jButton24.setVisible(false);
    Fenster3.jButton25.setVisible(false);
    Game.interagiert= true;
  } // end of jButton6_ActionPerformed
  
  public void jButton7_ActionPerformed(ActionEvent evt) {
    // TODO hier Quelltext einfügen
    Game.input=2;//Setzt input auf 2, womit die Nachfolgene Abfrage das Enthüllen des Knopfes selbst(jButton7) und das Enthüllen des Vorangengangenen(jButton6) zur Folge hat(Beschreibung hier fast identisch lediglich jew. ein Knopf mehr Pro gehaltener Karte)
    Fenster3.jButton6.setVisible(false);
    Fenster3.jButton7.setVisible(false);
    Fenster3.jButton8.setVisible(false);
    Fenster3.jButton9.setVisible(false);
    Fenster3.jButton10.setVisible(false);
